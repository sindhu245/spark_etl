DELETE FROM adtech_master_data.location_cluster_mapping WHERE cluster_id=(select id from adtech_master_data.location_cluster where targeting_tag = 'CLUSTER_HYD_V2') AND location_id=(select id from adtech_master_data.city where targeting_tag = 'CITY_ISLAMPUR');
DELETE FROM adtech_master_data.location_cluster_mapping WHERE cluster_id=(select id from adtech_master_data.location_cluster where targeting_tag = 'CLUSTER_PUN_V2') AND location_id=(select id from adtech_master_data.city where targeting_tag = 'CITY_ISLAMPUR');

INSERT INTO adtech_master_data.location_cluster_mapping (cluster_id, location_id) VALUES ((select id from adtech_master_data.location_cluster where targeting_tag = 'CLUSTER_KOL_V2'), (select id from adtech_master_data.city where targeting_tag = 'CITY_ISLAMPUR'));